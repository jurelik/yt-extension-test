{
  "manifest_version": 2,
  "name": "yt-test",
  "version": "1.0",

  "description": "just a test",

  "permissions": [
    "tabs",
    "contextMenus"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch?v=*"],
      "js": ["yt-test.js"]
    },
    {
      "matches": ["*://*.youtube.com/watch?v=*"],
      "css": ["yt-test.css"]
    }
  ]
}